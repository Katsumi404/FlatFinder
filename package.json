{
  "name": "flatfinder",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "ios": {
    "googleServicesFile": "./GoogleService-Info.plist"
  },
  "android": {
    "googleServicesFile": "./google-services.json"
  },
  "dependencies": {
    "@react-native-firebase/app": "17.4.0",
    "@react-native-firebase/auth": "17.4.0",
    "@react-native-firebase/database": "17.4.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "expo": "~50.0.14",
    "expo-build-properties": "~0.5.1",
    "expo-file-system": "^16.0.8",
    "expo-modules-autolinking": "~1.1.0",
    "expo-sample-pedometer": "0.2.0",
    "expo-sqlite": "^13.4.0",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "^2.9.3",
    "firebase": "^9.6.11",
    "react": "18.2.0",
    "react-native": "^0.73.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "react-native-dotenv": "^3.4.11"
  },
  "private": true,
  "plugins": [
    "./node_modules/expo-sample-pedometer/expo-sample-pedometer-plugins.js",
    "@react-native-firebase/app",
    [
      "expo-build-properties",
      {
        "ios": {
          "useFrameworks": "static"
        }
      }
    ]
  ]
}
